					"WHERE NUMSUP='",peel(taDb2Array["NUMSUP"]," "),"' "\
					"AND SEQSUP='",peel(taDb2Array["SEQSUP"]," "),"' "\
					"AND CODPRO='",peel(taDb2Array["CODPRO"]," "),"' "\
					"AND UVCSRV='",peel(taDb2Array["UVCSRV"]," "),"' WITH NONE")
					
