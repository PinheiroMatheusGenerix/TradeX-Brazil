						tNameOut := build("FM",tToInfologPrefixFilename,tCODACTLidoConvertido,"GISCONFSEP",time("now" ,"%Y%m%d%H%M%S"),"_",replace(taREFLIVSplitExp[1], " ", "_"),".txt")
