! Message definition

base "syslog.cfg" SYSLOG

!===========================================================

! Initialization section

begin
	SYSLOG := find(sSYSLOG,INDEX=number(pINDEX))
    tLine := read(SYSLOG.rp)
    while tLine <> EOF do
        if substr(tLine, 1, 22) = "INHTTP.FILE1_ORIG_NAME" then
            tOriginalName := strip(peel(substr(tLine, 24, EOL), " "), " ")
        endif
        tLine := read(SYSLOG.rp)
    endwhile
    close(SYSLOG.rp)

    tFileOut := build(sHOME,"/FMBR_AS2/",tOriginalName)
    close(tFileOut)
    copy(SYSLOG.a, tFileOut)
    close(tFileOut)

    log("O conte√∫do do arquivo foi copiado para ", tFileOut, NL)
endbegin

!===========================================================

! Default statements section
default

enddefault

!===========================================================

! End statements section

end

endend
